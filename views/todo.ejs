<!DOCTYPE html>
<html>
<head>
 <title>Doing</title>
 <link rel='stylesheet' href='/stylesheets/style.css' />
 <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">

</head>
<body>
    <h1><%= greeting %></h1>
    <ul>  
        <% tasks.forEach(function(task){ %>
        <li><%= task.due_date %></li> 
        <li><%= task.description %></li> 
        <li><%= task.title %></li> 
        <li><%=task.priority %></li>
        <li><%=task.complete %></li>
        <% }) %>
    </ul>
</body>
</html>